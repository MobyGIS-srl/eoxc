{
  "name": "eoxc",
  "version": "0.0.1",
  "description": "EOxC client generator",
  "main": "dist/eoxc.js",
  "scripts": {
    "build": "webpack -p",
    "build-dev": "webpack -d",
    "watch": "webpack -d --watch",
    "test": "mocha --compilers js:babel-register test/setup/node.js test/unit/**/*.js",
    "test:watch": "npm run test -- -w",
    "build-browser-tests": "webpack -d --config webpack.tests.config.js",
    "build-browser-tests:watch": "npm run build-browser-tests -- --watch",
    "dev-server": "webpack-dev-server --content-base test/apps/OpenLayers --watch",
    "docs": "jsdoc README.md src -d ./docs -r -P package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eoxc/eoxc.git"
  },
  "keywords": [],
  "author": "Fabian Schindler <fabian.schindler.strauss@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eoxc/eoxc/issues"
  },
  "homepage": "https://github.com/eoxc/eoxc",
  "devDependencies": {
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.8.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2016": "^6.0.11",
    "babel-register": "^6.5.1",
    "chai": "^3.4.1",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.10.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.7.0",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-react": "^5.1.1",
    "exports-loader": "^0.6.3",
    "file-loader": "^0.8.5",
    "glob": "^6.0.3",
    "handlebars-loader": "^1.3.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.3",
    "mocha": "^2.3.4",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9"
  },
  "dependencies": {
    "backbone": "^1.3.3",
    "backbone-nested": "^2.0.4",
    "backbone.marionette": "^2.4.5",
    "bootstrap": "^3.3.6",
    "d3": "^3.5.17",
